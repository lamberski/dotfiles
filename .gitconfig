[alias]
	s = status -s
	c = commit -m
  ca = commit --amend
  co = checkout
  a = add
  d = diff
	ds = diff --staged
  pop = "!git checkout master; git merge develop; git push origin master; git checkout develop"
  pos = "!git checkout staging; git merge develop; git push origin staging; git checkout develop"
  pod = "!git push origin develop"
  undo = reset --soft HEAD~1
  sync = "!git fetch origin; git remote prune origin"
